{ // Copy, and update this file as "config.json"
    "dataPath": "../data/data.json", // The location of the JSON file with the data for the analysis.

    "context":{ // Required. Context for the analysis used when prompting
        "items": "",  // Required. Single word to describe the items that will be analyzed.
        "purpose": "", // Required. A sentence that introduces the purpose of the research.
        "outcome": "" // Required. One-or-two words to describe what you want to learn from the items.

    },

    "verticals": [], // Optional. List of categories to produce labels, and scores.
    "labelPrompts": {}, // Optional. Overrides labeling prompt. Expects key-value object, with verticals as keys.

    "labels": null, // Not-supported. Use custom labels, not the ones generated by LLMs.
    "nComponents": 5, // Number of components when reduccing text embeddings.
    "itemsToLabel": 100, // How many items to sample when labeling data.
    "labelPrompt": "", // Optional. Overrides labeling prompt. See prompt.tsx for more info.

    "itemsToScore": 100, // How many items to sample when labeling data.
    "scorePrompt": "", // Optional. Overrides labeling prompt. See prompt.tsx for more info.
    "numClusters": 3, // Number of clusters for the analysis.
    "predictor": null // Not-supported. Reuses linear models to predict scores.
}
